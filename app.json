{
  "name": "plus Music Bot",
  "description": "Best Music bot to play songs in your Telegram's Group Voice Chat.",
  "keywords": ["music", "voicechat", "telegram"],
  "repository": "https://github.com/OFFICIALHACKERERA/PLUS-MUSIC-BOT",
  "logo": "https://te.legra.ph/file/f509648f758b0e47f53a1.jpg",
  "stack": "container",
  "env": {
    "SESSION_NAME": {
      "description": "Pyrogram session string",
      "required": true
    },
    "BOT_TOKEN": {
      "description": "A bot token from @BotFather",
      "required": true
    },
    "API_ID": {
      "description": "App ID from my.telegram.org/apps",
      "required": true
    },
    "API_HASH": {
      "description": "App hash from my.telegram.org/apps",
      "required": true
    },
    "DURATION_LIMIT": {
      "description": "Max audio duration limit for downloads (minutes) i suggest 20-30 min.",
      "required": true,
      "value": "200"
    }
  }
}
