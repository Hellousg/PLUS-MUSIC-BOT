{
  "name": "Music Bot",
  "description": "Open-Source & Best Music bot to play songs in your Telegram's Group Voice Chat. Powered by PyTgCalls.",
  "keywords": ["music", "voicechat", "telegram"],
  "repository": "https://github.com/LEGENDARY-OS/MUSIC-BOT",
  "stack": "container",
  "env": {
    "SESSION_NAME": {
      "description": "Pyrogram session string",
      "required": true
    },
    "BOT_TOKEN": {
      "description": "A bot token from @BotFather",
      "required": true
    },
    "BOT_NAME": {
      "description": "Your MusicPlayer Bot username without @.",
      "required": false,
      "value": "PLUS_MUSIC_BOT"
    },
    "API_ID": {
      "description": "App ID from my.telegram.org/apps",
      "required": true
      "value": "13612586"
    },
    "API_HASH": {
      "description": "App hash from my.telegram.org/apps",
      "required": true
      "value": "58611e0f0c1dcc1222b9ee191fa078bf"
    },
    "SUDO_USERS": {
      "description": "List of user IDs counted as admin everywhere (separated by space).",
      "required": true
      "value": "5096428460"
    },
    "MASTER_USERNAME": {
      "description": "Write Ur Username(Without @ Or With @).",
      "required": true
      "value": "OFFICIALHACKERERA"
    },
    "DURATION_LIMIT": {
      "description": "Max audio duration limit for downloads (minutes) i suggest 20-30 min.",
      "required": true,
      "value": "200"
    }
  }
}
